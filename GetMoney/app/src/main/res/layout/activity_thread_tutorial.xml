<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.ilink.pen.getmoney.activity.ThreadTutorialActivity">
    <Button
        android:id="@+id/btnStartMainThread"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="startMainThread"/>
    <Button
        android:id="@+id/btnStartThread01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="startThread01"/>
    <Button
        android:id="@+id/btnStartThread02"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="startThread02"/>
    <Button
        android:id="@+id/btnStartThread03"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="startThread03"/>

</LinearLayout>
